datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

model Word {
  word      String @id @default("") @map("word") @db.Text
  results   Json[] @default([]) @map("results") @db.JsonB
  frequency Float  @default(0) @map("frequency") @db.Real

  @@index([word])
  @@map("words")
}
